{
  "name": "kube-ec-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "codegen": "npm run codegen:ts",
    "codegen:ts": "openapi-typescript ../api/openapi/user.yaml -o ./src/types/api/user.ts",
    "codegen:watch": "npm run codegen:ts -- --watch"
  },
  "dependencies": {
    "next": "^15.0.0",
    "openapi-fetch": "^0.12.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "openapi-typescript": "^7.4.4",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  }
}
